{
    "trailingComma": "none",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 120,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "/routes/",
        "/middleware/",
        "^../(.*)",
        "^./(.*)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
